pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "MAVEN_HOME"
    }

    stages {
        stage('Preparation') {
            steps {
                https://github.com/arunarudra/selenium-Maven.git 
            }
        }
        stage('Build') {
            steps {
                //sh "mvn clean test"
                bat "mvn clean test"
            }
        }
    }

}
